<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.ssafy.happyhouse.model.mapper.HouseDealMapper">
   <select id="listhouse" resultType="HouseDealDto">
      select *
      from housedeal 
      where dong= #{key}
   </select>
   
   <select id="listlike" resultType="HouseDealDto">
      select *
      from housedeal 
      where dong = (select dong from LikeArea where userid=#{key})
   </select>
   
   <select id="listall" resultType="HouseDealDto">
      select *
      from housedeal
   </select>
   
   <select id="detail" resultType="HouseDealDto">
      select *
      from housedeal
      where no= #{no}
   </select>
</mapper>